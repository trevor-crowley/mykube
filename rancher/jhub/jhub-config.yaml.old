proxy:
  secretToken: "b86812ddeccccf87c14148f4ab629f7991da41e77d4a6bf470943f0131c24917"
singleuser:
  lifecycleHooks:
    postStart:
      exec:
        command: ["gitpuller", "https://github.com/HSCanada/datsci.git", "master", "datsci"]
  image:
    # Get the latest image tag at:
    # https://hub.docker.com/r/jupyter/datascience-notebook/tags/
    # Inspect the Dockerfile at:
    # https://github.com/jupyter/docker-stacks/tree/master/datascience-notebook/Dockerfile
    name: "tcrow.io:5000/scipy-notebook"
    tag: "latest"
  imagePullSecret:
    enabled: false
    registry: "tcrow.io:5000"
    username: "testuser"
    email: "testuser@tcrow.io"
    password: "testpassword"
  defaultUrl: "/lab"
  storage:
    capacity: "2Gi"
  extraEnv:
    EDITOR: "vim"
hub:
  extraConfig:
    jupyterlab: |
      c.Spawner.cmd = ['jupyter-labhub']
auth:
  type: github
  github:
    clientId: "7ed97ca90ea254bf5849"
    clientSecret: "2a751bcd93ab0071257d097eaaf3e915a57fc95b"
    callbackUrl: "https://jhub.tcrow.io/hub/oauth_callback"
